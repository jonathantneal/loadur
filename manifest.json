{
	"name": "Loadur",
	"description": "Choose your bandwidth destiny",
	"version": "1.0",
	"manifest_version": 2,
	"background": {
		"scripts":["background.js"]
	},
	"browser_action": {
		"default_icon": "icon-38.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"*://s.imgur.com/*"
	],
	"icons": {
		"32": "icon-32.png",
		"128": "icon-128.png"
	}
}
